/**
 * Demultiplexor:
 * {a, b} = {in, 0} if sel == 0
 *          {0, in} if sel == 1
 */

CHIP DMux {
    IN in, sel;
    OUT a, b;

    PARTS:
    Nand(a=sel, b=sel, out=nandSel);
    Nand(a=in, b=nandSel, out=nand1);
    Nand(a=sel, b=in, out=nand2);
    Nand(a=nand1, b=nand1, out=a);
    Nand(a=nand2, b=nand2, out=b);
}
