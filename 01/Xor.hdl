/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=b, out=nandAB);
    Nand(a=a, b=nandAB, out=nand1);
    Nand(a=b, b=nandAB, out=nand2);
    Nand(a=nand1, b=nand2, out=out);
}